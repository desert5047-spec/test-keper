{
  "cli": {
    "version": ">= 16.30.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "environment": "preview",
      "distribution": "internal",
      "ios": { "simulator": false },
      "android": { "buildType": "apk" },
      "env": {
        "EXPO_PUBLIC_ENV": "stg",
        "EXPO_PUBLIC_STORAGE_BUCKET": "test-images"
      }
    },
    "production": {
      "environment": "production",
      "autoIncrement": true,
      "ios": { "simulator": false },
      "android": { "buildType": "app-bundle" },
      "env": {
        "EXPO_PUBLIC_ENV": "prod",
        "EXPO_PUBLIC_STORAGE_BUCKET": "test-images"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
